class Node {
	field int dataX;
	field int dataY;
	field Node next;

	constructor Node new(int dataXIn, int dataYIn, Node nextNode){
		let dataX = dataXIn;
		let dataY = dataYIn;
		let next = nextNode;
		return this;
	}

	method void draw(){
		do Screen.setColor(false);
		do Screen.drawPixel(dataX, dataY);
		return;
	}

	method void dispose(){
		do Memory.deAlloc(this);
		return;
	}

	method Node getNext(){
		return next;
	}

	method void setNext(Node newNext){
		let next = newNext;
		return;
	}
}