class Bike{
	//field variables
	field int x;
	field int y;
	
	//constructor
	constructor Bike new(int xPos, int yPos){
		let x = xPos;
		let y = yPos;
		return this;
	}

	//move
	//method void move(){

	//}

	//dispose

	//erase

	//draw
	method void draw(){ //up
		do Screen.drawLine(x, y-2, x-2, y+2);
		do Screen.drawLine(x-2, y+2, x+2, y+2);
		do Screen.drawLine(x+2, y+2, x, y-2);
		return;
	}
}